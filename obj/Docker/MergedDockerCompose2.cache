services:
  accemo:
    build:
      context: C:\Users\WebDeveloper\source\repos\Xacton
      dockerfile: Accemo/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Accemo
      target: base
    container_name: Accemo_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: accemo:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net5.0/Accemo.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\WebDeveloper\source\repos\Xacton\Accemo:/app:rw
    - C:\Users\WebDeveloper\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\WebDeveloper\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\WebDeveloper\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Program Files (x86)\Microsoft Visual Studio\Shared\NuGetPackages:/root/.nuget/fallbackpackages:ro
    - C:\Users\WebDeveloper\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\WebDeveloper\source\repos\Xacton:/src:rw
version: '3.4'